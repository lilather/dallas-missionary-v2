doctype html
html 
  head
    meta(charset='UTF-8')  
    block title&description
    meta(name='viewport', content='width=device-width')
    // Include All CSS here
    link(rel='stylesheet',  href="https://use.typekit.net/qyr4iik.css")
    link(href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous")     
    each link in links
     link(rel='stylesheet' href=link)
    // End Include All CSS
    // Favicon Icon
    link(rel='icon', type='image/png', href='images/favicon.png')
  body(id=id)
   block bodyTag 
  #header 
    nav.navbar.navbar-expand-xl.navbar-light
      .container-fluid
        a.navbar-brand(href='#' title='Dallas Missionary Church')
            | Dallas
            | Missionary Church 
        button.navbar-toggler(type='button' data-bs-toggle='collapse' data-bs-target='#navbarSupportedContent' aria-controls='navbarNavDropdown' aria-expanded='false' aria-label='Toggle navigation')
          span.navbar-toggler-icon
        #navbarSupportedContent.collapse.navbar-collapse
          ul.navbar-nav.ms-auto
            li.nav-link
              a.link(title='Home' aria-current='page' href='#') home
            li.nav-link
              a.link(href='#' title='About us') about
            li.nav-link
              a.link(href='#' title=' We have 5 ways to give') giving
            li.nav-link
              a.link(href='#' title=' Our charitable minsitries') ministries
            li.nav-link
              a.link(href='#' title=' Contact Us') contact
          
  block banner
  block content
  block footer 
           footer.footer
            p Copyright Â© | Dallas Missionary Church  2021 All Rights Reserved.

  block scripts 
     each script in scripts
      script(src=script)
